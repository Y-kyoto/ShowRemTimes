{
  "content_scripts":[
    {
      "matches":[
        "https://www.pixiv.net/*"
      ],
      "js":[
        "jquery-3.4.0.min.js",
        "main.js"
      ],
      "css": [
        "css/menu.css",
        "css/bootstrap.min.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": {
      "19": "icon/19.png"
    }
  },
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "description": "pixivのメニュー表示を変更するための拡張機能です。",
  "manifest_version": 2,
  "name": "pixivのメニュー表示を変更する",
  "permissions": ["storage", "activeTab"],
  "version": "1"
}
